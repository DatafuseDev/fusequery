statement ok
CREATE TABLE IF NOT EXISTS t1(id Int, v Vector) Engine = Fuse

statement ok
INSERT INTO t1 (id, v) VALUES(1, [1.1, 1.2, 1.3]),(2, [2.1, 2.2, 2.3]),(3, [3.1, 3.2, 3.3]);

query IT
select * from t1
----
1 [1.1,1.2,1.3]
2 [2.1,2.2,2.3]
3 [3.1,3.2,3.3]

query II
select v[1], v[2] from t1
----
1.1 1.2
2.1 2.2
3.1 3.2

query I
select cosine_distance(v, [10.1,11.2,12.3]) from t1;
----
0.99992687
0.9990788
0.9985147

