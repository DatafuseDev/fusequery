statement ok
DROP DATABASE IF EXISTS db20;

statement ok
CREATE DATABASE db20;

statement ok
USE db20;

statement ok
CREATE TABLE IF NOT EXISTS t13 (a INT);

statement ok
INSERT INTO t13 VALUES (1), (2), (3);

query I
SELECT * FROM t13 ORDER BY a;
----
1
2
3

statement ok
SET enable_query_result_cache = 1;

query I
SELECT * FROM t13 ORDER BY a;
----
1
2
3

statement ok
INSERT INTO t13 VALUES (4), (5), (6);

statement ok
SET enable_query_result_cache = 0;

query I
SELECT * FROM t13 ORDER BY a;
----
1
2
3
4
5
6

statement ok
SET enable_query_result_cache = 1;

statement ok
SET tolerate_inconsistent_result_cache = 1;

query I
SELECT * FROM t13 ORDER BY a;
----
1
2
3

statement ok
SET tolerate_inconsistent_result_cache = 0;

query I
SELECT * FROM t13 ORDER BY a;
----
1
2
3
4
5
6

statement ok
DROP TABLE t13;

statement ok
DROP DATABASE db20;
