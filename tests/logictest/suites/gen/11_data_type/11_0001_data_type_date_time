statement query T
select '1022-05-16T03:25:02.000000+08:00'::TIMESTAMP;

----
1022-05-15 19:25:02.000000

statement query T
select '1000-01-01T03:25:02.868894-0700'::TIMESTAMP;

----
1000-01-01 10:25:02.868894

statement query T
select '9999-05-06T03:25:02.868894-07:00'::TIMESTAMP;

----
9999-05-06 10:25:02.868894

statement query T
select '9999-12-12T03:25:02.868894+0000'::TIMESTAMP;

----
9999-12-12 03:25:02.868894

statement query B
select '2021-01-01T00:00:00+0000'::timestamp = '2021-01-01T01:00:00+0100'::timestamp;

----
1

statement ok
drop database if exists db1;

statement ok
create database db1;

statement ok
create table db1.t1(id int, a datetime);

statement ok
insert into db1.t1 values(1, '2022-01-01 02:00:11'), (2, '2022-01-02T02:00:22'), (3, '2022-02-02T04:00:03+0000'), (4, '2022-03-02T01:12:00-07:00')

statement query IT
select * from db1.t1;

----
1  2022-01-01 02:00:11.000000
2  2022-01-02 02:00:22.000000
3  2022-02-02 04:00:03.000000

statement ok
set timezone='Asia/Shanghai';

statement query IT
select * from db1.t1;

----
1  2022-01-01 10:00:11.000000
2  2022-01-02 10:00:22.000000
3  2022-02-02 12:00:03.000000

statement ok
drop database db1;

statement ok
set timezone='UTC';

